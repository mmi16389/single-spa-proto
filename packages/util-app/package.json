{
  "scripts": {
    "serve": "webpack-dev-server --port 3000",
    "build": "webpack --mode=production",
    "lint": "eslint src --ext js,ts,tsx",
    "analyze": "webpack --mode=production --env.analyze=true",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "format": "prettier --write './**'",
    "start:standalone": "webpack-dev-server --env.standalone",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch"
  },
  "dependencies": {
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.8.7",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-ts-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-cli": "^25.2.7",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "systemjs-webpack-interop": "^2.1.2",
    "ts-config-single-spa": "^1.7.0",
    "typescript": "^3.8.3",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-ts": "^1.4.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:test npm:lint"
    }
  }
}
